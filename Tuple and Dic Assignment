a=[(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]
def last(n):
    return n[-1]  
   
def sort(tuples):
    return sorted(tuples, key=last)
   
print(sort(a))



-------------------------------------------------------------------

my_dict = {} # Create the dict
 
# Now populate it
for i in range(97, 97 + 26):
    # Map character to ascii value
    my_dict[chr(i)] = i
 
print(my_dict)
